<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.afive.wecheck.article.mapper.ArticleFileMapper">

	<select id="getList" resultType="org.afive.wecheck.article.bean.ArticleBean">
		SELECT	*
		FROM quote;
	</select>

    <select id="get" parameterType="String" resultType="org.afive.wecheck.article.bean.ArticleBean">
    	SELECT	*
    	FROM	quote
    	WHERE	quoteID = #{quoteID};
    </select>
    
    <select id="getByArticleID" parameterType="String" resultType = "org.afive.wecheck.article.bean.ArticleBean">
  		SELECT	*
  		FROM	articleFile
  		WHERE	articleID = #{articleID};
  	</select>
</mapper>
